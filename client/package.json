    "@hot-loader/react-dom": "^16.8.6",
    "@testing-library/react": "^8.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^5.16.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
